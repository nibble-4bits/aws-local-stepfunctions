{
  "extends": "@tsconfig/node16-strictest/tsconfig.json",
  "compilerOptions": {
    "outDir": "build",
    "declaration": true,
    "noUncheckedIndexedAccess": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "stripInternal": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
